			<condition attribute="emailaddress1" operator="eq" value="test" />
		</filter>
		<link-entity name="campaign" from="campaignid" to="campaignid" >
			<attribute name="new_mailrelaygroupid" alias="campaign_new_mailrelaygroupid" />
